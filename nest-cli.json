{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/streamapp/src",
  "monorepo": true,
  "root": "apps/streamapp",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/streamapp/tsconfig.app.json"
  },
  "projects": {
    "streamapp": {
      "type": "application",
      "root": "apps/streamapp",
      "entryFile": "main",
      "sourceRoot": "apps/streamapp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/streamapp/tsconfig.app.json"
      }
    },
    "recommend": {
      "type": "application",
      "root": "apps/recommend",
      "entryFile": "main",
      "sourceRoot": "apps/recommend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/recommend/tsconfig.app.json"
      }
    },
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "content": {
      "type": "application",
      "root": "apps/content",
      "entryFile": "main",
      "sourceRoot": "apps/content/src",
      "compilerOptions": {
        "tsConfigPath": "apps/content/tsconfig.app.json"
      }
    }
  }
}